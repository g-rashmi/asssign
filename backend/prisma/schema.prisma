

generator client {
  provider = "prisma-client-js"
 
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}




model Product {
  id         Int       @id @default(autoincrement())
  name       String
  feedbacks  Feedback[]
}

model Feedback {
  id         Int       @id @default(autoincrement())
  rating     Int?     
  review     String?   
  createdAt  DateTime  @default(now())

  email      String
   image     String? 
  product  Product  @relation(fields:[productId],references:[id])
  productId  Int
  
  @@unique([email,productId])
}
